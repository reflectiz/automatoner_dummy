{
  "compileOnSave": true,
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strictBindCallApply": true,
    "target": "ES2017",
    "baseUrl": ".",
    "allowUnreachableCode": true
  },
  "exclude": [
    "node_modules"
  ]
}